{
  "name": "scriptsp",
  "version": "0.1.0",
  "description": "Run multiple package.json scripts in parallel (for development)",
  "main": "index.js",
  "bin": {
    "scriptsp": "bin/scriptsp"
  },
  "files": [
    "bin",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smallhelm/scriptsp.git"
  },
  "keywords": [
    "task",
    "tool",
    "package.json",
    "parallel",
    "npm"
  ],
  "author": "smallhelm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smallhelm/scriptsp/issues"
  },
  "homepage": "https://github.com/smallhelm/scriptsp#readme",
  "scripts": {
    "test": "bin/scriptsp dev;bin/scriptsp dev2;bin/scriptsp error;bin/scriptsp colors",
    "dev:server": "echo \"da\" && sleep 1 && echo \"server\"",
    "dev:client": "echo \"da\" && sleep 1 && echo \"client\"",
    "dev:bot": "echo \"da\" && sleep 1 && echo \"bot\"",
    "dev": [
      "dev:server",
      "dev:bot",
      "dev:client"
    ],
    "dev2": [
      "dev:server",
      "dev:server",
      "dev:server",
      "dev:bot",
      "dev:client"
    ],
    "error": [
      "dev",
      "dev2",
      "colors"
    ],
    "colors": [
      "dev:server",
      "dev:server",
      "dev:server",
      "dev:server",
      "dev:server",
      "dev:server",
      "dev:server",
      "dev:server",
      "dev:bot",
      "dev:bot",
      "dev:bot",
      "dev:bot",
      "dev:bot",
      "dev:bot",
      "dev:client",
      "dev:client",
      "dev:client",
      "dev:client",
      "dev:client",
      "dev:client"
    ]
  },
  "dependencies": {
    "chalk": "1.0.0",
    "lodash": "3.9.3",
    "x-is-array": "0.1.0"
  }
}
